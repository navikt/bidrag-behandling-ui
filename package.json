{
  "name": "bidrag-behandling-ui",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ts": "tsc --build",
    "lint": "eslint ./src/**/*.{js,ts,tsx} package.json --quiet",
    "fix-lint": "eslint ./src/**/*.{js,ts,tsx} package.json --quiet --fix",
    "test": "cross-env NODE_ENV=TEST mocha --config .mocharc.js --spec ./src/__tests__/**/**/*.spec.{js,ts,tsx} --exit",
    "start": "",
    "env:export:local": "export $(cat env/.env.local | xargs)",
    "dev": "cross-env-shell NODE_ENV=development webpack serve --config webpack.dev.config.js",
    "dev:mock": "cross-env ENABLE_MOCK=true NODE_ENV=development webpack serve --config webpack.dev.config.js",
    "dev:local": "cross-env ENABLE_MOCK=true NODE_ENV=development webpack serve --env envfile=.env.local.private --config webpack.dev.config.js",
    "build": "webpack --config webpack.prod.config.js",
    "build:api": "npm-run-all -p api:person api:bidragGrunnlag api:bidragVedtak api:bidragBehandling",
    "api:person": "swagger-typescript-api -p https://bidrag-person.dev.intern.nav.no/bidrag-person/v3/api-docs --axios -n PersonApi.ts -o src/api",
    "api:bidragGrunnlag": "swagger-typescript-api -p https://bidrag-grunnlag.intern.dev.nav.no/v3/api-docs --axios -n BidragGrunnlagApi.ts -o src/api",
    "api:bidragVedtak": "swagger-typescript-api -p https://bidrag-vedtak.intern.dev.nav.no/v3/api-docs --axios -n BidragVedtakApi.ts -o src/api",
    "api:bidragBehandling": "swagger-typescript-api -p https://bidrag-behandling.intern.dev.nav.no/v3/api-docs --extract-enums --enum-names-as-values --axios -n BidragBehandlingApiV1.ts -o src/api",
    "api:bidragBehandling:local": "swagger-typescript-api -p http://localhost:8990/v3/api-docs --extract-enums --enum-names-as-values --axios -n BidragBehandlingApiV1.ts -o src/api",
    "api:notat": "swagger-typescript-api -p http://0.0.0.0:8580/v3/api-docs --extract-enums --axios -n BidragDokumentProduksjonApi.ts -o src/api",
    "api:bidragInntekt": "swagger-typescript-api -p https://bidrag-inntekt.intern.dev.nav.no/v3/api-docs --extract-enums --axios -n BidragInntektApi.ts -o src/api"
  },
  "dependencies": {
    "@navikt/aksel-icons": "5.12.4",
    "@navikt/bidrag-ui-common": "^1.10.9",
    "@navikt/ds-css": "5.12.4",
    "@navikt/ds-css-internal": "3.4.3",
    "@navikt/ds-react": "5.12.4",
    "@navikt/ds-react-internal": "3.4.3",
    "@navikt/ds-tailwind": "5.12.4",
    "@navikt/ds-tokens": "^5.12.4",
    "@tanstack/react-query": "^5.17.9",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/lodash": "^4.14.202",
    "@unleash/proxy-client-react": "^4.1.1",
    "echarts": "^5.4.3",
    "express": "^4.18.2",
    "lodash.debounce": "^4.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.12",
    "react-hook-form": "^7.49.3",
    "react-router-dom": "^6.21.1",
    "unleash-proxy-client": "^3.3.0"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/mocha": "^10.0.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^7.0.2",
    "autoprefixer": "^10.4.16",
    "chai": "^4.3.10",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.35.0",
    "cross-env": "^7.0.3",
    "cross-fetch": "^4.0.0",
    "css-loader": "^6.8.1",
    "dotenv-webpack": "^8.0.1",
    "esbuild-loader": "4.0.2",
    "esbuild-register": "^3.5.0",
    "eslint": "^8.56.0",
    "eslint-config-eslint": "^9.0.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "esm": "^3.2.25",
    "file-loader": "^6.2.0",
    "global-jsdom": "^9.2.0",
    "html-webpack-plugin": "^5.6.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^23.0.1",
    "mini-css-extract-plugin": "^2.7.6",
    "mocha": "^10.2.0",
    "msw": "1.3.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.32",
    "postcss-import": "15.1.0",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.1.1",
    "process": "^0.11.10",
    "react-refresh": "^0.14.0",
    "sinon": "^17.0.1",
    "swagger-typescript-api": "^13.0.3",
    "tailwindcss": "^3.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-manifest-plugin": "^5.0.0",
    "webpack-merge": "^5.10.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
