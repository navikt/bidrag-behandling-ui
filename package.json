{
  "name": "bidrag-behandling-ui",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ts": "tsc --build",
    "lint": "eslint './src/**/*.{js,ts,tsx}' package.json",
    "fix-lint": "eslint './src/**/*.{js,ts,tsx}' package.json --quiet --fix",
    "test": "cross-env NODE_ENV=TEST mocha --config .mocharc.js --spec ./src/__tests__/**/**/*.spec.{js,ts,tsx} --exit",
    "start": "",
    "serve": "npx serve -l 5253 ./dist",
    "env:export:local": "export $(cat env/.env.local.private | xargs)",
    "dev": "cross-env-shell NODE_ENV=development webpack serve --config webpack.dev.config.mjs",
    "dev:q1": "cross-env-shell NODE_ENV=development webpack serve --env envfile=.env.local.q1 --config webpack.dev.config.mjs",
    "dev:mock": "cross-env ENABLE_MOCK=true NODE_ENV=development webpack serve --config webpack.dev.config.mjs",
    "dev:local": "cross-env ENABLE_MOCK=true NODE_ENV=development webpack serve --env envfile=.env.local.private --config webpack.dev.config.mjs",
    "dev:localq1": "cross-env ENABLE_MOCK=true NODE_ENV=development webpack serve --env envfile=.env.localq1.private --config webpack.dev.config.mjs",
    "build": "webpack --config webpack.prod.config.mjs",
    "build:api": "npm-run-all -p api:person api:bidragGrunnlag api:bidragVedtak api:bidragBehandling",
    "api:person": "swagger-typescript-api generate -p https://bidrag-person-q2.dev.intern.nav.no/bidrag-person/v3/api-docs --axios -n PersonApi.ts -o src/api",
    "api:bidragGrunnlag": "swagger-typescript-api generate -p https://bidrag-grunnlag.intern.dev.nav.no/v3/api-docs --axios -n BidragGrunnlagApi.ts -o src/api",
    "api:bidragVedtak": "swagger-typescript-api generate -p https://bidrag-vedtak-q2.intern.dev.nav.no/v3/api-docs --axios -n BidragVedtakApi.ts -o src/api",
    "api:bidragBehandling": "swagger-typescript-api generate -p https://bidrag-behandling-q2.intern.dev.nav.no/v3/api-docs --extract-enums --enum-names-as-values --axios -n BidragBehandlingApiV1.ts -o src/api",
    "api:bidragBehandling:local": "swagger-typescript-api generate -p http://localhost:8990/v3/api-docs --extract-enums --enum-names-as-values --axios -n BidragBehandlingApiV1.ts -o src/api",
    "api:notat": "swagger-typescript-api generate -p http://localhost:8183/v3/api-docs --extract-enums --axios -n BidragDokumentProduksjonApi.ts -o src/api",
    "api:bidragAdmin": "swagger-typescript-api generate -p https://bidrag-admin.intern.dev.nav.no/v3/api-docs --extract-enums --enum-names-as-values --axios -n BidragAdminApiV1.ts -o src/api"
  },
